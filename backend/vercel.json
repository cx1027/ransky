{
    "version": 2,
    "builds": [
        {
            "src": "app/main.py",
            "use": "@vercel/python"
        }
    ],
    "routes": [
        {
            "src": "/(.*)",
            "dest": "app/main.py"
        }
    ],
    "functions": {
        "api/**": {
        "excludeFiles": "{.next,*.cache,node_modules,public,app}/**"
        }
    }
    
      
}